{
    "id": "qgis-s3",
    "version": "3.34.4",
    "description": "Run an interactive QGIS session on Stampede3.",
    "owner": "${apiUserId}",
    "enabled": true,
    "runtime": "ZIP",
    "runtimeVersion": null,
    "runtimeOptions": null,
    "containerImage": "tapis://cloud.data/corral/tacc/aci/CEP/applications/v3/interactive-template/interactive.zip",
    "jobType": "BATCH",
    "maxJobs": -1,
    "maxJobsPerUser": -1,
    "strictFileInputs": true,
    "jobAttributes": {
        "description": "",
        "dynamicExecSystem": false,
        "execSystemConstraints": null,
        "execSystemId": "stampede3",
        "execSystemExecDir": "${JobWorkingDir}",
        "execSystemInputDir": "${JobWorkingDir}",
        "execSystemOutputDir": "${JobWorkingDir}",
        "execSystemLogicalQueue": "skx-dev",
        "archiveSystemId": "stampede3",
        "archiveSystemDir": "HOST_EVAL($WORK)/tapis-jobs-archive/${JobCreateDate}/${JobName}-${JobUUID}",
        "archiveOnAppError": true,
        "isMpi": false,
        "mpiCmd": null,
        "cmdPrefix": null,
        "parameterSet": {
            "appArgs": [],
            "containerArgs": [],
            "schedulerOptions": [
                {
                    "name": "TACC Scheduler Profile",
                    "description": "Scheduler profile for HPC clusters at TACC",
                    "inputMode": "FIXED",
                    "arg": "--tapis-profile tacc-apptainer",
                    "notes": {
                        "isHidden": true
                    }
                },
                {
                    "name": "TACC Interactive Session Substrings",
                    "description": "VNC and DCV sessions require the substrings 'tap_' and '-dcvserver' in the slurm job name in order to function.",
                    "inputMode": "FIXED",
                    "arg": "--job-name ${JobName}-dcvserver-tap_dsmydata",
                    "notes": {
                        "isHidden": true
                    }
                }
            ],
            "envVariables": [
                {
                    "key": "_XTERM_CMD",
                    "value": "apptainer run docker://qgis/qgis:final-3_34_4 qgis --profiles-path $STOCKYARD/qgis --code /work/projects/wma_apps/stampede2/qgis/set_favorite_dir.py --noversioncheck",
                    "description": null,
                    "inputMode": "FIXED",
                    "notes": {
                        "label": "XTERM Program CMD",
                        "isHidden": true
                    }
                }
            ],
            "archiveFilter": {
                "includes": [],
                "excludes": [
                    "qgis*.zip"
                ],
                "includeLaunchFiles": true
            }
        },
        "fileInputArrays": [],
        "nodeCount": 1,
        "coresPerNode": 48,
        "memoryMB": 192000,
        "maxMinutes": 120,
        "subscriptions": [],
        "tags": []
    },
    "tags": [
        "portalName: CEP",
        "portalName: DesignSafe"
    ],
    "notes": {
        "label": "QGIS (Stampede3)",
        "helpUrl": "https://www.designsafe-ci.org/user-guide/tools/visualization/qgis/",
        "hideNodeCountAndCoresPerNode": true,
        "icon": "QGIS",
        "category": "Data Processing",
        "isInteractive": true
    }
}
