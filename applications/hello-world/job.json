{
    "name": "hello-world-test",
    "appId": "hello-world",
    "appVersion": "0.0.1",
    "description": "test hello-world job 1",
    "fileInputs": [
        {
            "name": "File to modify",
            "sourceUrl": "tapis://cloud.corral.community/work/05089/sal/test/in.txt",
            "targetPath": "in.txt"
        }
    ],
    "parameterSet": {
        "appArgs": [
            {
                "name": "Greeting",
                "arg": "greetings"
            },
            {
                "name": "Target",
                "arg": "earthling"
            }
        ],
        "schedulerOptions": [
            {
                "name": "TACC Allocation",
                "description": "The TACC Allocation associated with this job execution",
                "include": true,
                "arg": "-A TACC-ACI"
            }
        ]
    },
    "tags": [
        "test"
    ],
    "subscriptions": [
        {
            "description": "Email notification",
            "enabled": false,
            "eventCategoryFilter": "JOB_NEW_STATUS",
            "ttlMinutes": 1,
            "deliveryTargets": [
                {
                    "deliveryMethod": "WEBHOOK",
                    "deliveryAddress": "$WH_BASE_URL/webhooks/jobs/"
                }
            ]
        }
    ]
}
